# EditorConfig (http://EditorConfig.org) helps developers define and maintain consistent
# coding styles between different editors and IDE's.

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
